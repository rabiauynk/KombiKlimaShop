
@{
    ViewData["Title"] = "CreateCategory";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<div class="content">
	<div class="container-fluid">
		<h4 class="page-title">Bayi Kategori İşlemleri </h4>
		<div class="row">
			<div class="col-md-12">
				<div class="card">
					<form method="post">
						<div class="card">
				         	<div class="card-header">
						       <div class="card-title">Yeni Kategori Girişi</div>
					        </div>
					<div class="card-body">
						<div class="form-group">
							<label for="dealercategoryname">Bayi Kategorisinin Adını Giriniz</label>
								<input type="text" class="form-control" id="dealercategoryname" >
						</div>
					
					</div>
					<div class="card-action">
						<button class="btn btn-primary">Bayi Kategorisini Ekle</button>
				
					</div>
				</div>
				</form>
			</div>
		</div>
	</div>
</div>



<!-- Header Start -->
<div class="container-fluid bg-breadcrumb">
    <div class="container text-center py-5" style="max-width: 900px;">
        <h4 class="text-white display-4 mb-4 wow fadeInDown" data-wow-delay="0.1s">Tanıtım Videoları</h4>
        <ol class="breadcrumb d-flex justify-content-center mb-0 wow fadeInDown" data-wow-delay="0.3s">
            <li class="breadcrumb-item"><a href="index.html">Anasayfa</a></li>
            <li class="breadcrumb-item active text-primary">Tanıtım Videoları</li>
        </ol>
    </div>
</div>
<!-- Header End -->
<!-- video Start --> 
<div class="container p-5">
    <div class="video-gallery" style="display: flex; flex-wrap: wrap; gap: 20px;">
        @foreach (var videos in Model)
        {
            <div class="video-item" style="flex: 1 1 30%;">
                <iframe width="100%" height="315" src="@videos.VideoUrl" frameborder="0" allowfullscreen></iframe>
            </div>
        }
    </div>
</div>

            }
<!-- video End -->